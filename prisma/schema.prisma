// npx prisma db seed
// npx prisma migrate dev

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id            String    @id @default(cuid())
    name          String
    password      String
    email         String    @unique
    emailVerified DateTime?
    image         String?
}
